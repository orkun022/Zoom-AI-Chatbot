# Zoom App Credentials (from Zoom Marketplace > App Credentials)
ZOOM_CLIENT_ID=your_client_id
ZOOM_CLIENT_SECRET=your_client_secret
ZOOM_ACCOUNT_ID=your_account_id
ZOOM_REDIRECT_URL=https://your-cloudflare-url/oauth/callback

# Bot Identity (from Zoom Marketplace > Features > Team Chat)
BOT_JID=your_bot_jid@xmpp.zoom.us

# Maistro API (optional - for custom NLP backend)
MAISTRO_SESSION_URL=https://your-maistro-endpoint/api/v1/agent/label/YOUR_LABEL/session
MAISTRO_MESSAGE_URL=https://your-maistro-endpoint/api/v1/session/{sessionId}/message

# Server
PORT=5000
